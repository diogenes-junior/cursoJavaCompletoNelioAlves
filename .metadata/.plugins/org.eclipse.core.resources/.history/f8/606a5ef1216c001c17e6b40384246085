import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);

		double A,B,C;
		
		A = sc.nextDouble();
		B = sc.nextDouble();
		C = sc.nextDouble();
		
		if(A>=B+C || B>=A+C || C>=A+B)
			System.out.println("NAO FORMA TRIANGULO");
		
		sc.close();
	}

}
