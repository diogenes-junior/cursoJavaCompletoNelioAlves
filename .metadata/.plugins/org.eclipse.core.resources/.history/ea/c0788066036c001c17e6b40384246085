import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n1, n2, n3, aux;
		
		n1 = sc.nextInt();
		n2 = sc.nextInt();
		n3 = sc.nextInt();
		
		if(n1>n2) {
			aux=n1;
			n1=n2;
			n2=aux;
		}
		if(n1>n3) {
			aux=n1;
			n1=n3;
			n3=aux;
		}
		if(n2>n3) {
			aux=n2;
			n2=n3;
			n3=aux;			
		}
		
		System.out.println(n1);
		System.out.println(n2);
		System.out.println(n3);
		
		sc.close();
	}

}
